<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.comcel.com.co/consultarParametrosBB/"
	xmlns:tns="http://www.comcel.com.co/consultarParametrosBB/">
	
	<xs:element name="Input_Parameters" type="tns:Input_Paramaters" />
	<xs:element name="WS_Result" type="tns:WS_Result" />

	<xs:complexType name="Input_Paramaters">
		<xs:sequence>
			<xs:element name="OFFER_BB" type="xs:string" /> 					
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="WS_Result">
		<xs:sequence>
			<xs:element name="CODIGO" type="xs:string" />
			<xs:element name="MENSAJE" type="tns:respuestaType" />
			<xs:element name="DESCRIPCION" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="respuestaType">
		<xs:sequence>
			<xs:element name="VIGENCIA" type="xs:string" />	
			<xs:element name="CODIGO_ACTIVACION_RIM" type="xs:string" />	
			<xs:element name="CODIGO_DESACTIVACION_RIM" type="xs:string" />	
			<xs:element name="RESULTADO" type="xs:string" />						
		</xs:sequence>
	</xs:complexType>

</xs:schema>